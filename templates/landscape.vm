        <div class="row row-1">
            <div class="column center">
                <h1>Metaverse landscape</h1>
                <span id="subline">Overview of the current state of the metaverse space, featuring relevant projects and technologies</span>
            </div>
        </div>
        <div class="filter-container">
            <div id="toggle-extended-container">
                <label class="toggle-tooltip">
                    Show extended landscape
                    <span class="tooltip">Extended landscape shows projects and technologies that do not market with metaverse explicitly</span>
                </label>
                <input id="toggle-extended" type="checkbox" />
            </div>
            <div id="more-information">
                <input type="text" id="search" placeholder="Search for name or company" />
                <span>Click on a logo to see more information!</span>
            </div>
        </div>
        <div class="row row-1" style="margin-top: 0px;">
            <div class="column">
                <div id="projects" class="layer-wrapper">
                    <div class="layer-text">
                        Projects
                    </div>
                    <div class="layer">
                        #foreach( $layer in $projectsView.keySet() )
                        <div class="capability-layer">
                            <div class="capability">$layer</div>
                            <div class="capability-items">
                                #foreach ($project in $projectsView.get($layer))
                                <div
                                    class="item"
                                    data-name="$project.name"
                                    data-twitter-handle="$project.twitterHandle"
                                    data-category="$project.category"
                                    data-status="$project.status"
                                    data-website="$project.website"
                                    data-metaverse-native="$project.metaverseNative"
                                    data-three-d="$project.threeD"
                                    data-open-world="$project.openWorld"
                                    data-user-content="$project.userContent"
                                    data-access="$project.access"
                                    data-token="$project.token"
                                    data-tech-stack="$project.techStack"
                                    data-description="$project.description"
                                >
                                    #if( $project.logoData )
                                    <img src="$project.logoData"/>
                                    #end
                                </div>
                                #end
                            </div>
                        </div>
                        #end
                    </div>
                </div>
                <hr id="framework-divider"/>
                #foreach($layer in $technologiesView.keySet())
                <div id="$layer.toLowerCase()" class="layer-wrapper">
                    <div class="layer-text">
                        $layer
                    </div>
                    <div class="layer">
                        #foreach ($capability in $technologiesView.get($layer).keySet())
                        <div class="capability-layer">
                            <div class="capability">$capability</div>
                            <div class="capability-items">
                                #foreach ($technology in $technologiesView.get($layer).get($capability))
                                <div
                                    class="item"
                                    data-name="$technology.name"
                                    data-description="$technology.description"
                                    data-website="$technology.website"
                                    data-categories="$technology.categories"
                                    data-company="$technology.company"
                                    data-metaverse-native="$technology.metaverseNative"
                                    data-twitter-handle="$technology.twitterHandle"
                                    data-github="$technology.github"
                                >
                                    #if( $technology.logoData )
                                    <img src="$technology.logoData" />
                                    #end

                                </div>
                                #end
                            </div>
                        </div>
                        #end
                    </div>
                </div>
                #end
            </div>
