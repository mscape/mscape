    <div id="modal-background"></div>
    <script>
        var modalBackground = document.querySelector("#modal-background");
        var modalWrapper = document.querySelector("#modal-wrapper");
        var modal = document.querySelector("#modal");
        var modalContent = document.querySelector("#modal-content");
        var modalClose = document.querySelector("#modal-close");
        var toggleExtended = document.querySelector("#toggle-extended");
        var extendedItems = document.querySelectorAll("[data-metaverse-native='0']");
        var framework = JSON.parse('$framework');
        var items = document.querySelectorAll('.item');

        function lookUpFrameworkLayer(lookUpCapability) {
            var layers = framework.Technologies;
            for (layer in layers) {
                for (capability of layers[layer]) {
                    if (capability == lookUpCapability) {
                        return layer;
                    }
                }
            }
        }

        toggleExtended.addEventListener("change", function(event) {
            if(toggleExtended.checked) {
                extendedItems.forEach(element => {
                    element.style.display = "flex";
                });
            } else {
                extendedItems.forEach(element => {
                    element.style.display = "none";
                });
            }
        });

        function closeModal () {
            modal.style.display = "none";
            modalBackground.style.display = "none";
            modalWrapper.style.display = "none";
        }

        modalWrapper.addEventListener('click', function(event) {
            if(event.target.id == "modal-wrapper") {
                closeModal();
            }
        });

        modalClose.addEventListener('click', function() {
            closeModal();
        });

        items.forEach(item => {
            item.addEventListener('click', function(event) {
                modalBackground.style.display = "block";
                modalWrapper.style.display = "flex";
                modal.style.display = "block";
                var element = event.currentTarget;
                var logo = element.querySelector('img, svg');
                var tempHtml = ''

                if (logo) {
                    tempHtml += '<div id="modal-logo">' + logo.outerHTML + '</div>';
                }

                tempHtml += '<h3 style="margin-top: 30px;">' + element.dataset.name + '</h3>';
                tempHtml += '<p>' + element.dataset.description + '</p>'

                tempHtml += '<table>';

                if(element.dataset.company) {
                    tempHtml += '<tr><th>Company</th><td>' + element.dataset.company + '</td></tr>';
                }

                if(element.dataset.layer) {
                    tempHtml += '<tr><th>Layer</th><td>' + element.dataset.layer + '</td></tr>';
                }

                if(element.dataset.categories) {
                    var categories = element.dataset.categories.replace("]","").replace("[","").split(",");
                    categories = categories.map( category => category.trim());
                    console.log(categories);
                    tempHtml += '<tr><th>Categories</th><td>';
                    categories.forEach( category => {
                        tempHtml += '<div class="modal-pill modal-pill-' + lookUpFrameworkLayer(category).toLowerCase() + '">' + category + '</div>';
                    })
                    tempHtml += '</td></tr>';
                }

                if (element.dataset.category) {
                    tempHtml += '<tr><th>Category</th><td><div class="modal-pill modal-pill-project">' + element.dataset.category + '</div></td></tr>';
                }

                if(element.dataset.metaverseNative == "1") {
                    tempHtml += '<tr><th>Metaverse Native</th><td>Yes</td></tr>';
                } else if(element.dataset.metaverseNative == "0") {
                    tempHtml += '<tr><th>Metaverse Native</th><td>No</td></tr>';
                }

                if(element.dataset.website) {
                    tempHtml += '<tr><th>Website</th><td><a href="' + element.dataset.website + '" target="_blank">' + element.dataset.website + '</a></td></tr>';
                }

                if(element.dataset.status) {
                    tempHtml += '<tr><th>Status</th><td>' + element.dataset.status + '</td></tr>';
                }

                if(element.dataset.access) {
                    var access = element.dataset.access.replace("]","").replace("[","")
                    tempHtml += '<tr><th>Access</th><td>' + access + '</td></tr>';
                }

                if(element.dataset.token) {
                    tempHtml += '<tr><th>Token</th><td>' + element.dataset.token + '</td></tr>';
                }

                if(element.dataset.threeD == "1") {
                    tempHtml += '<tr><th>3D world</th><td>Yes</td></tr>';
                } else if(element.dataset.threeD == "0") {
                    tempHtml += '<tr><th>3D world</th><td>No</td></tr>';
                }

                if(element.dataset.openWorld == "1") {
                    tempHtml += '<tr><th>Open world</th><td>Yes</td></tr>';
                } else if(element.dataset.openWorld == "0") {
                    tempHtml += '<tr><th>Open world</th><td>No</td></tr>';
                }

                if(element.dataset.userContent == "1") {
                    tempHtml += '<tr><th>User generated content</th><td>Yes</td></tr>';
                } else if(element.dataset.userContent == "0") {
                    tempHtml += '<tr><th>User generated content</th><td>No</td></tr>';
                }

                tempHtml += '</table>';

                if(element.dataset.github) {
                    tempHtml += '<a href="https://github.com/' + element.dataset.github + '" target="_blank" class="github-image"><img src="https://gh-card.dev/repos/' + element.dataset.github + '.svg" /></a>';
                }

                if(element.dataset.twitterHandle) {
                    tempHtml += '<a class="twitter-timeline" data-width="100%" data-height="300" data-theme="dark" href="https://twitter.com/' + element.dataset.twitterHandle + '">Tweets by ' + element.dataset.twitterHandle + '</a>';
                    twttr.widgets.load();
                }

                modalContent.innerHTML = tempHtml;
            });
        });

        window.addEventListener("keydown", function(event) {
            if(event.key == "Escape") {
                closeModal();
            }
        }, true);

        var typingTimer;
        var TYPING_DONE_INTERVAL = 600;
        var inputSearch = document.querySelector("#search");

        inputSearch.addEventListener("keyup", function(event) {
            clearTimeout(typingTimer);
            typingTimer = setTimeout(search, TYPING_DONE_INTERVAL, inputSearch.value);
        });

        function search(value) {
            value = value.toLowerCase();
            
            items.forEach(item => {
                var name = item.dataset.name.toLowerCase();
                var company = item.dataset.company ? item.dataset.company.toLowerCase() : "";
                
                if(value == "") {
                    if(toggleExtended.checked == 1 || (toggleExtended.checked == 0 && item.dataset.metaverseNative == 1)) {
                        item.style.display = 'flex';
                    }
                } else if(name.indexOf(value) !== -1 || company.indexOf(value) !== -1) {
                    item.style.display = 'flex';
                } else {
                    item.style.display = 'none';
                }
            });
        }
    </script>
    <script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>
